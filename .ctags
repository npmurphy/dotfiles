--langmap=c++:+.cl *
--langdef=latex
--langmap=latex:.tex.ltx.latex"
--regex-latex=/\\tableofcontents/TABLE OF CONTENTS/s,tableofcontents/
--regex-latex=/\\frontmatter/FRONTMATTER/s,frontmatter/
--regex-latex=/\\mainmatter/MAINMATTER/s,mainmatter/
--regex-latex=/\\backmatter/BACKMATTER/s,backmatter/
--regex-latex=/\\begin{thebibliography}/BIBLIOGRAPHY/s,thebibliography/
--regex-latex=/\\appendix/APPENDIX/s,appendix/
--regex-latex=/\\begin{abstract}/Abstract/s,abstract/
--regex-latex=/\\part[ \t]*\*?\{[ \t]*([^}]*)\}/\1/s,part/
--regex-latex=/\\chapter[ \t]*\*?\{[ \t]*([^}]*)\}/\1/s,chapter/
--regex-latex=/\\section[ \t]*\*?\{[ \t]*([^}]*)\}/\1/s,section/
--regex-latex=/\\subsection[[:space:]]*\*?\{[ \t]*([^}]*)\}/+ \1/s,subsection/
--regex-latex=/\\subsubsection[ \t]*\*?\{[ \t]*([^}]*)\}/+  \1/s,subsubsection/
--regex-latex=/\\frametitle[ \t]*\*?\{[ \t]*([^}]*)\}/-   \1/s,slide/
--regex-latex=/\\begin\{block\}[ \t]*\*?\{[ \t]*([^}]*)\}/+ \1/s,slide/
--regex-latex=/\\paragraph[ \t]*\*?\{[ \t]*([^}]*)\}/+   \1/s,paragraph/
--regex-latex=/\\subparagraph[ \t]*\*?\{[ \t]*([^}]*)\}/+    \1/s,subparagraph/
--regex-latex=/\\label[ \t]*\*?\{[ \t]*([^}]*)\}/\1/l,label/
--regex-latex=/\\ref[ \t]*\*?\{[ \t]*([^}]*)\}/\1/r,ref/
